<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<link href="${pageContext.request.contextPath}/userjsp/admin/css/train.css" rel="stylesheet" type="text/css">
</head>
<body>
	<div>
	<h3>列车添加</h3>
	<hr/>
	<form action="#" method="post">
		<table>
			<tr><td>车次编号：</td><td><input type="text" name="id"/></td></tr>
			<tr><td>起始　站：</td>
			<td>
				<select name="startstation">
					<option value="GZ">广州</option>
				</select>
			</td>
			</tr>
			<tr><td>终到　站：</td>
			<td>
				<select name="endstation">
						<option value="BJ">北京</option>
				</select>
			</td>
			</tr>
			<tr><td>起始时间：</td><td><input type="date" name="starttime"/></td></tr>
			<tr><td>终到时间：</td><td><input type="date" name="endtime"/></td></tr>
			<tr><td>里　　程：</td><td><input type="text" name="mile"/></td></tr>
			<tr><td>无座票数量：</td><td><input type="text" name="noseatnumber"/></td></tr>
			<tr><td>车辆车体分类：</td><td><input type="text" name="bodytype"/></td></tr>
			<tr><td>列车类型：</td><td><input type="text" name="type"/></td></tr>
			<tr><td>列车状态：</td><td><input type="text" name="con1"/></td></tr>
			<tr><td><input type="submit" value="添加"/>&nbsp;&nbsp;&nbsp;<input type="reset" value="重置"/></td></tr>
		</table>
	</form>
	</div>
</body>
</html>